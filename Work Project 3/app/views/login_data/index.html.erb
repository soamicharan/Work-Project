<p id="notice"><%= notice %></p>

<h1>Login Data</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @login_data.each do |login_datum| %>
      <tr>
        <td><%= login_datum.username %></td>
        <td><%= link_to 'Show', login_datum %></td>
        <td><%= link_to 'Edit', edit_login_datum_path(login_datum) %></td>
        <td><%= link_to 'Destroy', login_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Login Datum', new_login_datum_path %>
